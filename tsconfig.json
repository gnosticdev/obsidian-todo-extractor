{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES6",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "Node",
		"noEmit": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"isolatedModules": true,
		"types": ["obsidian", "bun"],
		"strictNullChecks": true,
		"noUncheckedIndexedAccess": true,
		"lib": ["DOM", "ES5", "ES6", "ES7"],
		"jsx": "preserve",
		"jsxImportSource": "react"
	},
	"exclude": ["node_modules"],
	"include": ["src", "__tests__"]
}
